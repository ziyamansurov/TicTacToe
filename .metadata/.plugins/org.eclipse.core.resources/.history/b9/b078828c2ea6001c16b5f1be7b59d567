
public class TicTacToeMain {

	public static void main(String[] args) {

		char[][] gameBoard = { { ' ', '|', ' ', '|',' '} ,
				{ '-', '+','-', '+', '-' },
				{ ' ', '|', ' ', '|',' ' },
				{ '-', '+', '-', '+','-' },
				{ ' ', '|', ' ', '|',' ' }};
		
		for(int i=0;i<3;i++) {
			for(int j=0;j<4;j++) {
				System.out.print(gameBoard[i][j]);
			}
			System.out.println();
		}

	}

}
